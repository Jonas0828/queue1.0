<view class="weui-form-preview">
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell">
      <view class="weui-cell__bd" style="color: #55AAAD;">{{bankInfo.DotName}}</view>
    </view>
  </view>


  <view class="bankimage">
    <image src="../resource/picture/main/9.jpg" style="width:80%; height:90px;" />
  </view>
  <view class="weui-form-preview__bd">
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">详细地址</view>
      <view class="weui-form-preview__value">{{bankInfo.RegionName}}</view>
    </view>
    <view class="weui-form-preview__item">
      <view class="weui-form-preview__label">电话</view>
      <view class="weui-form-preview__value">0356-88888888</view>
    </view>
  </view>
<view class="btn" hidden="{{toggle}}">
  <view class="service" style="margin-left: 0%;" bindtap="getnumber">
    <view class="textoficon">对私</view>
  </view>
  <view class="service" bindtap="getnumber">
    <view class="textoficon">对公</view>
  </view>
  <view class="service" bindtap="getnumber">
    <view class="textoficon">VIP</view>
  </view>
</view>

  <view class="ticketinfo" hidden="{{!toggle}}" style="margin-top: 50rpx">
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell">
        <view class="weui-cell__bd" style="color: #55AAAD;">排队信息</view>
      </view>
    </view>
    <view class="weui-form-preview__bd" style="text-align: left">
      <view class="weui-form-preview__item">
        <view class="weui-form-preview__label">号码</view>
      </view>
      <view class="weui-form-preview__item">
        <view class="weui-form-preview__value" style="text-align: center;font-size: 30px;">{{ticketInfo.number}}</view>
      </view>
      <view class="weui-form-preview__item">
        <view class="weui-form-preview__label">日期</view>
        <view class="weui-form-preview__value">{{ticketInfo.date}}</view>
      </view>
      <view class="weui-form-preview__item">
        <view class="weui-form-preview__label">提示</view>
        <view class="weui-form-preview__value">前面还有{{ticketInfo.wait}}人，请耐心等待</view>
      </view>
      <view class="weui-form-preview__item">
        <view class="weui-form-preview__label">提示</view>
        <view class="weui-form-preview__value">请准备好身份证、银行卡或存折等所需证件</view>
      </view>
    </view>
  </view>
  <view class="weui-cells weui-cells_after-title" style="{{toggle ? '' :  'margin-top: 50rpx'}}">
    <view class="weui-cell">
      <view class="weui-cell__bd" style="color: #55AAAD;">个人业务</view>
    </view>
  </view>

  <view class="weui-grids">
    <view wx:for="{{gridsPerson}}" wx:key="*this" class="weui-grid" hover-class="weui-grid_active" bindtap="opentrade" data-index="{{index}}">
      <image class="weui-grid__icon" src="../resource/picture/bank/2.png" />
      <view class="weui-grid__label" style="color: rgba(0,0,0,0.5);">{{item.name}}</view>
    </view>

  </view>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell">
      <view class="weui-cell__bd" style="color: #55AAAD;">对公业务</view>
    </view>
  </view>

  <view class="weui-grids">
    <view wx:for="{{gridsCompany}}" wx:key="*this" class="weui-grid" hover-class="weui-grid_active" bindtap="" data-index="{{index}}">
      <image class="weui-grid__icon" src="../resource/picture/bank/2.png" />
      <view class="weui-grid__label" style="color: rgba(0,0,0,0.5);">{{item.name}}</view>
    </view>

  </view>
  <view class="weui-demo-dialog {{istrue ? 'weui-demo-dialog_show' : ''}}">
    <view class="weui-mask" bindtap="closeDialog"></view>
    <view class="weui-dialog__wrp" bindtap="closeDialog">
      <view class="weui-dialog" catchtap="stopEvent">
        <view class="weui-dialog__hd">
          <view class="weui-dialog__title" style="font-size: 30px;color: #55AAAD;font-weight:normal;">取号成功</view>
        </view>
        <view class="weui-dialog__bd">

          <!-- ---------------------------------------------- -->
          <view class="weui-form-preview__bd">
            <view class="weui-form-preview">
              <!-- <view class="weui-form-preview__hd">
                <view class="weui-form-preview__item">
                  <view class="weui-cell__bd" style="text-align: center;font-size: 30px"></view>
                </view>
              </view> -->
              <view class="weui-form-preview__bd" style="text-align: left">
                <view class="weui-form-preview__item">
                  <view class="weui-form-preview__label">号码</view>
                </view>
                <view class="weui-form-preview__item">
                  <view class="weui-form-preview__value" style="text-align: center;font-size: 30px;">{{ticketInfo.number}}</view>
                </view>
                <view class="weui-form-preview__item">
                  <view class="weui-form-preview__label">日期</view>
                  <view class="weui-form-preview__value">{{ticketInfo.date}}</view>
                </view>
                <view class="weui-form-preview__item">
                  <view class="weui-form-preview__label">提示</view>
                  <view class="weui-form-preview__value">前面还有4人，请耐心等待</view>
                </view>
                <view class="weui-form-preview__item">
                  <view class="weui-form-preview__label">提示</view>
                  <view class="weui-form-preview__value">请准备好身份证、银行卡或存折等所需证件</view>
                </view>
              </view>
            </view>
          </view>


          <!-- ---------------------------------------------- -->

        </view>
        <view class="weui-dialog__ft">
          <view class="weui-dialog__btn" bindtap="closeDialog" style="color: #55AAAD;">确定</view>
        </view>
      </view>
    </view>
  </view>
</view>