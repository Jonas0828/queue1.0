page {
  height: 100%;
  width: 100%;
  overflow-y:hidden;

}

::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
}

.displayinline{
  display: flex;
  align-items: center;
  flex-direction: column;
}
