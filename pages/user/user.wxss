.fullview{
  height: 100%;
  width: 100%;
}

.userinfo{
  width: 100%;
  height: 30%;
  background-color: #55AAAD;
  border-bottom-left-radius: 50rpx 30rpx;
  border-bottom-right-radius: 50rpx 30rpx;
}

.headview{
  margin-top: 5rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow:hidden;
}

.dearname{
  color: #FFFFFF;
  font-size: 12pt;
  margin-top: 5rpx;
}

.nearbankname{
  color: #FFFFFF;
  font-size: 10pt;
  margin-top: 5rpx;
}

.centerview{
  width: 100%;
  height: 55%;
  background-color: #FAFAFA;
}

.bottomview{
  display: flex;
  align-items: center; 
  justify-content: center;
  width: 100%;
  height: 15%;
  /* background-color: #FAFAFA; */
  background-color: pink;
}

.reserve{
  display: flex;
  align-items: center; 
  justify-content: center;
  width: 90%;
  height: 100rpx;
  background-color: #55AAAD;
  border-radius: 15rpx 15rpx;  
  font-size: 18pt;
  color: #FFFFFF;
}

.listview{
  top: 25%;
  left: 5%;
  width: 90vw;
  height: 60%;
  /* height: 765rpx; */
  background-color: #FAFAFA;
  /* background-color: yellow; */
  position: absolute;
  border-radius: 5rpx;
}

.itemA{
  width:100%;
  height: 25%;
  background-color: #FFFFFF;
  border-radius: 5rpx;
  box-shadow: 0rpx 10rpx 50rpx 10rpx rgba(85, 85, 85, 0.10);
}

.card{
  width: 100%;
  height: 50%;
}

.cardtop{
  display: flex;
  align-items: center;
  flex-direction: row;
}

#cardno{
  width: 70%;
  height: 100%;
  display: flex;
  align-items: flex-end;
  justify-content: flex-start;
  flex-direction: row;
  margin-left: 5%;
  color: #55AAAD;
}

#detailinfo{
  width: 30%;
  height: 100%;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  flex-direction: row;
  font-size: 12pt;
  color: #55AAAD;
}

.cardbottom{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  flex-direction: row;
  margin-left: 5%;
  color: #55AAAD;
  font-size: 11pt;
}

.itemB{
  width:100%;
  height: 65%;
  background-color: #FFFFFF;
  margin-top: 5%;
  border-radius: 5rpx;
  box-shadow: 0rpx 10rpx 50rpx 10rpx rgba(85, 85, 85, 0.10);
}

