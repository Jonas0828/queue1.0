page{
  background-color: #F5F6F7;
}

.tabdata{
  height: 88rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.tabitem{
  width: 25%;
  height: inherit;
  background-color:#FFFFFF;
  color: #353535;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
  font-size: 32rpx;
}

.tab{
  color: #1C6CEF;
}

.tab-border{
  background-color: #1C6CEF;
}

.selec_title{
  margin-bottom: 18rpx;
}

.select_border{
  background-color: #FFFFFF;
  width: 96rpx;
  height: 4rpx;
}

.tab_border{
  background-color: #1C6CEF;
}

.content{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.preview_form{
  margin-top: 20rpx;
  border-radius: 10rpx;
  width: 710rpx;
  display: flex;
  flex-direction: column;
  background-color: #FFFFFF;
  padding-bottom: 44rpx;
}

.preview_item{
  display: flex;
  align-items: flex-start;
  margin-left: 29rpx;
  margin-top: 24rpx;
}

.preview_item:nth-child(1){
  margin-left: 20rpx;
  margin-top: 26rpx;
  justify-content: flex-end;
}

.preview_label_head{
  width: 580rpx;
  font-size: 30rpx;
  color: #353535;
  font-weight: bold;
}
.preview_value_head{
  width: 90rpx;
  height: 42rpx;
  font-size: 30rpx;
  margin-right: 20rpx;
}

.preview_border{
  width: 670rpx;
  height: 0.86rpx;
  background-color: #F0F0F0;
  margin-left: 20rpx;
  margin-top: 26rpx;
}

.preview_item:nth-child(2){
  margin-top: 26rpx;
}

.preview_item:nth-child(3){
  margin-top: 22rpx;
}

.preview_label{
  width: 120rpxx;
  height: 42rpx;
  font-size: 30rpx;
  color: #888888;
}

.preview_value{
  width: 541rpx;
  text-align: right;
  color: #353535;
}

.bank_msg{
  margin-top: 30rpx;
  color: #888888;
  font-size: 28rpx;
}





